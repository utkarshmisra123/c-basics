#include <iostream>

using namespace std;

int main()
{
    int max,min,i,n=0;
    int arr[50];
    cin>>n;
    
    for(int i=0;i<n;i++)
       {
       std::cin >>arr[i];
       }
       max=arr[0];
       min=arr[0];
       for(int i=1;i<n;i++)
       {
       if(arr[i]>max)
       {
           max=arr[i];
        
       }
       if(arr[i]<min)
       {
           min=arr[i];
       }
       }
       cout<<"max is "<<max;
       cout<<"min is "<<min;
       return 0;

}
